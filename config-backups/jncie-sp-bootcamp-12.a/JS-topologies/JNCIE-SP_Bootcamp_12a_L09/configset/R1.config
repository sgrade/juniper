    host-name R1;
    root-authentication {
        encrypted-password "$1$dGp5qt.O$1dwcWRUqZLjZx5Js6lnID0"; ## SECRET-DATA
    }
    login {
        user lab {
            uid 2002;
            class super-user;
            authentication {
                encrypted-password "$1$OpUof7yc$MuD5dgjbSbYuYf/7TWdMv/"; ## SECRET-DATA
            }
        }
        user regress {
            uid 928;
