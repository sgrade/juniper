interfaces {
    ge-0/0/1 {
        description "Connection to R2";
        unit 0 {
            family inet {
                address 172.27.0.6/30;
            }
            family mpls;
        }
    }
    ge-0/0/2 {
        description "VPLS CE1";
    }
    ge-0/0/3 {
        description "L3VPN CE2";
        unit 0 {
            family inet {
                address 172.27.0.41/30;
            }
        }
    }
    ge-0/0/4 {
        description "connection to R5";
        unit 0 {
            family inet {
                address 172.27.0.21/30;
            }
            family mpls;
        }
    }
    ge-0/0/5 {
        description "Connection to R3";
        unit 0 {
            family inet {
                address 172.27.0.18/30;
            }
            family mpls;
        }
    }
    ge-0/0/6 {
        description "AE to R1";
    }
    ge-0/0/7 {
        description "AE to R1";
    }
    lo0 {
        unit 0 {
            family inet {
                address 172.27.255.4/32;
            }
        }
    }
}
protocols {
    mpls {
        interface all;
    }
    bgp {
        group IBGP {
            type internal;
            local-address 172.27.255.4;
            local-as 1;
            neighbor 172.27.255.1;
            neighbor 172.27.255.3;
            neighbor 172.27.255.2;
            neighbor 172.27.255.5;
        }
    }
    ospf {
        area 0.0.0.0 {
            interface all;
            interface ge-0/0/0.0 {
                disable;
            }
        }
    }
    ldp {
        interface all;
    }
}
security {
    forwarding-options {
        family {
            inet6 {
                mode packet-based;
            }
            mpls {
                mode packet-based;
            }
            iso {
                mode packet-based;
            }
        }
    }
}
