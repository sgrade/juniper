interfaces {
    ge-0/0/1 {
        description "Connection to R1";
        unit 0 {
            family inet {
                address 172.27.0.13/30;
            }
            family mpls;
        }
    }
    ge-0/0/2 {
        description "Connection to R4";
        unit 0 {
            family inet {
                address 172.27.0.17/30;
            }
            family mpls;
        }
    }
    ge-0/0/3 {
        description "Connection to R5";
        unit 0 {
            family inet {
                address 172.27.0.26/30;
            }
            family mpls;
        }
    }
    ge-0/0/4 {
        description "Connection to Customer router";
        unit 0 {
            family inet {
                address 172.27.0.57/30;
            }
        }
    }
    lo0 {
        unit 0 {
            family inet {
                address 172.27.255.3/32;
            }
        }
    }
}
protocols {
    mpls {
        interface all;
    }
    bgp {
        group IBGP {
            type internal;
            local-address 172.27.255.3;
            local-as 1;
            neighbor 172.27.255.1;
            neighbor 172.27.255.4;
            neighbor 172.27.255.5;
            neighbor 172.27.255.2;
        }
    }
    ospf {
        area 0.0.0.0 {
            interface all;
            interface ge-0/0/0.0 {
                disable;
            }
        }
    }
    ldp {
        interface all;
    }
}
security {
    forwarding-options {
        family {
            inet6 {
                mode packet-based;
            }
            mpls {
                mode packet-based;
            }
            iso {
                mode packet-based;
            }
        }
    }
}
