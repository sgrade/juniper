groups {
    ae {
        interfaces {
            <ae*> {
                unit 0 {
                    family iso;
                    family mpls;
                }
            }
        }
    }
}
apply-groups ae;
system {
    host-name R5;
    root-authentication {
        encrypted-password "$1$8ssVjZsm$kRo89vwqwFhdmfXpdMnuA/"; ## SECRET-DATA
    }
    login {
        user lab {
            uid 2002;
            class super-user;
            authentication {
                encrypted-password "$1$5w8f03Iz$hNTMYtS1nEcT3pFkj4B/l1"; ## SECRET-DATA
            }
        }
    }
    services {
        ftp;
        ssh;
        telnet;
    }
    syslog {
        file messages {
            any critical;
        }
    }
}
interfaces {
    ge-0/0/1 {
        description "Connection to R3";
        mtu 1514;
        unit 0 {
            description CORE;
            family inet {
                address 172.27.0.25/30;
            }
            family iso;
            family mpls;
        }
    }
    ge-0/0/3 {
        description "L3VPN CE1";
        unit 0 {
            family inet {
                address 172.27.0.45/30;
            }
        }
    }
    ge-0/0/4 {
        description "Connection to customer router";
        unit 0 {
            family inet {
                address 172.27.0.49/30;
            }
        }
    }
    ge-0/0/5 {
        description "Connection to transit router";
        unit 0 {
            family inet {
                address 172.27.0.57/30;
            }
            family inet6 {
                address 2008:4498::39/126;
            }
        }
    } 
    ge-0/0/6 {
        description "Connection to internal server";
        unit 0 {
            family inet {
                address 172.27.155.6/24;
            }
        }
    }
    ge-0/0/7 {
        description "Connection to R4 AE2";
    }
    ge-0/0/8 {
        description "Connection to R4 AE2";
    }
    ge-0/0/9 {
        description "VRRP connection to DC3";
    }
    lo0 {
        unit 0 {
            family inet {
                address 172.27.255.5/32;
            }
            family iso {
                address 49.0172.0027.0255.0005.00;
            }
        }
    }
}
routing-options {
    autonomous-system 3895077211;
}
protocols {
    rsvp {
        interface all;
    }
    mpls {
        interface all;
    }
    bgp {
        group int {
            type internal;
            local-address 172.27.255.5;
            family inet {
                any;
            }
            family inet-vpn {
                any;
            }
            family inet6 {
                any;
            }
            family l2vpn {              
                signaling;
            }
            neighbor 172.27.255.1;
            neighbor 172.27.255.2;
        }
    }
    isis {
        interface all {
            level 1 disable;
        }
    }
    ospf {
        area 0.0.0.0 {
            interface ge-0/0/5.0;
        }
    }
    ldp {
        interface all;
    }
}
security {
    forwarding-options {
        family {
            inet6 {
                mode packet-based;
            }
            mpls {
                mode packet-based;
            }
            iso {
                mode packet-based;
            }
        }
    }
}