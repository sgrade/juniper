groups {
    ae {
        interfaces {
            <ae*> {
                unit 0 {
                    family iso;
                    family mpls;
                }
            }
        }
    }
}
apply-groups ae;
system {
    host-name R3;
    root-authentication {
        encrypted-password "$1$muHljhcM$yRFjP2AjofzFk8ZcCPgK4."; ## SECRET-DATA
    }
    login {
        user lab {
            uid 2002;
            class super-user;
            authentication {
                encrypted-password "$1$5w8f03Iz$hNTMYtS1nEcT3pFkj4B/l1"; ## SECRET-DATA
            }
        }
    }
    services {
        ftp;
        ssh;
        telnet;
    }
    syslog {
        file messages {
            any critical;
        }
    }
}
interfaces {
    ge-0/0/1 {
        description "Connection to R1";
        mtu 1514;
        unit 0 {
            description CORE;
            family inet {
                address 172.27.0.13/30;
            }
            family iso;
            family mpls;
        }
    }
    ge-0/0/2 {
        description "Connection to R4";
        mtu 1514;
        unit 0 {
            description CORE;
            family inet {
                address 172.27.0.17/30;
            }
            family iso;
            family mpls;
        }
    }
    ge-0/0/3 {
        description "Connection to R5";
        mtu 1514;
        unit 0 {
            description CORE;
            family inet {
                address 172.27.0.26/30;
            }
            family iso;
            family mpls;
        }
    }
    ge-0/0/4 {
        description "VRRP connection to DC3";
    }
    ge-0/0/5 {
        description "Connection to internal server";
        unit 0 {
            family inet {
                address 172.27.155.4/24;
            }
        }
    }
    lo0 {
        unit 0 {
            family inet {
                address 172.27.255.3/32;
            }
            family iso {
                address 49.0172.2702.5500.0300;
            }
        }
    }
}
routing-options {
    autonomous-system 3895077211;
}
protocols {
    rsvp {
        interface all;
    }
    mpls {
        interface all;
    }
    bgp {
        group int {
            type internal;
            local-address 172.27.255.3;
            family inet {
                any;
            }
            family inet-vpn {
                any;
            }
            family inet6 {
                any;
            }
            family l2vpn {              
                signaling;
            }
            neighbor 172.27.255.1;
            neighbor 172.27.255.2;
        }
    }
    isis {
        interface all {
            level 1 disable;
        }
    }
    ldp {
        interface all;
    }
}
security {
    forwarding-options {
        family {
            inet6 {
                mode packet-based;
            }
            mpls {
                mode packet-based;
            }
            iso {
                mode packet-based;
            }
        }
    }
}