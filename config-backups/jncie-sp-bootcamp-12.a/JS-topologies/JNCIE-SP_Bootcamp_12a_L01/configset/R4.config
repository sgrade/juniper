groups {
    ae {
        interfaces {
            <ae*> {
                unit 0 {
                    family iso;
                    family mpls;
                }
            }
        }
    }
}
apply-groups ae;
system {
    host-name R4;
    root-authentication {
        encrypted-password "$1$qoqf.6Q9$koG9qVmkLNlmsy7AG6VfT/"; ## SECRET-DATA
    }
    login {
        user lab {
            uid 2002;
            class super-user;
            authentication {
                encrypted-password "$1$5w8f03Iz$hNTMYtS1nEcT3pFkj4B/l1"; ## SECRET-DATA
            }
        }
    }
    services {
        ftp;
        ssh;
        telnet;
    }
    syslog {
        file messages {
            any critical;
        }
    }
