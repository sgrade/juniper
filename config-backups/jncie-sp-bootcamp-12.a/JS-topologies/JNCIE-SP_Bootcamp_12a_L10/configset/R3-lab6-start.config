interfaces {
    ge-0/0/1 {
        description "Connection to R1";
        unit 0 {
            family inet {
                address 172.27.0.13/30;
            }
        }
    }
    ge-0/0/2 {
        description "Connection to R4";
        unit 0 {
            family inet {
                address 172.27.0.17/30;
            }
        }
    }
    ge-0/0/3 {
        description "Connection to R5";
        unit 0 {
            family inet {
                address 172.27.0.26/30;
            }
        }
    }
    ge-0/0/4 {
        description "Connection to Customer";
        unit 0 {
            family inet {
                address 172.27.0.57/30;
            }
        }
    }
    lo0 {
        unit 0 {
            family inet {
                address 172.27.255.3/32;
            }
        }
    }
}
routing-options {
    autonomous-system 3895077211;
}
protocols {
    bgp {
        group internal {
            type internal;
            local-address 172.27.255.3;
            export nhs;
            neighbor 172.27.255.1;
            neighbor 172.27.255.2;
            neighbor 172.27.255.4;
            neighbor 172.27.255.5;
        }
        group customer {
            type external;
            neighbor 172.27.0.58 {
                peer-as 64176;
            }
        }
    }
    ospf {
        traffic-engineering;
        reference-bandwidth 10g;
        area 0.0.0.0 {
            interface ge-0/0/1.0;
            interface ge-0/0/2.0;
            interface ge-0/0/3.0;
            interface lo0.0;
        }
    }
}
policy-options {
    policy-statement nhs {
        from {
            protocol bgp;
            neighbor 172.27.0.58;
        }
        then {
            next-hop self;
            accept;
        }
    }
}
